<script type="text/javascript">
    function TreeList_BeginCallback(s, e) {
        e.customArgs['RowVersions'] = s.cpRowVersions;
    }

    function TreeList_EndCallback(s, e) {
        if (s.cpEditError) {
            alert('Error: ' + s.cpEditError);
            delete s.cpEditError;
        }
    }
</script>

@Html.Action("TreeListPartial")